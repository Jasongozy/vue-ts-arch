  "private": true,
  "license": "UNLICENSE",
  "scripts": {
    "build": "build",
    "start": "gulp serve",
    "serve": "gulp serve -d",
    "test": "rimraf test_results && jest --no-cache --no-coverage",
    "coverage": "rimraf test_results && jest --no-cache"
  },
  "engines": {
    "node": ">=8.2.1",
    "npm": ">=5.3.0"
  },
  "dependencies": {
    "av-ts": "^0.9.0",
    "axios": "^0.16.2",
    "axios-retry": "^2.0.1",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chalk": "^0.4.31",
    "@types/clear-require": "^2.0.0",
    "@types/core-js": "^0.9.42",
    "@types/debug": "^0.0.29",
    "@types/del": "^2.2.32",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/gulp": "^4.0.4",
    "@types/gulp-connect": "^5.0.0",
    "@types/gulp-util": "^3.0.31",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/http-proxy": "^1.12.0",
    "@types/jest": "^20.0.5",
    "@types/lodash": "^4.14.71",
    "@types/lodash-webpack-plugin": "^0.11.1",
    "@types/meow": "^3.6.2",
    "@types/node": "^8.0.19",
    "@types/run-sequence": "0.0.29",
    "@types/webpack": "^3.0.5",
    "@types/webpack-dev-server": "^2.4.1",
    "ajv": "^5.2.2",
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "clear-require": "^2.0.0",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "jest-localstorage-mock": "^1.2.0",
    "lodash-webpack-plugin": "^0.11.4",
    "meow": "^3.7.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.3.0-alpha.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "run-sequence": "^2.1.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "time-stamp": "^2.0.0",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "uglify-es": "^3.0.27",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.6.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": false,
        "useBabelrc": true
      }
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts"
    },
    "setupFiles": [
      "babel-polyfill/lib/index.js",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
